<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >

    <Button
        android:id="@+id/btn_widget"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="控件"
        />

    <Button
        android:id="@+id/btn_custom_drawing"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="自定义绘图"
        />

    <Button
        android:id="@+id/btn_use_database"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="数据库的使用"
        />

    <Button
        android:id="@+id/btn_permission"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="6.0权限管理"
        />

    <Button
        android:id="@+id/btn_call_system_function"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="调用系统功能"
        />

    <Button
        android:id="@+id/btn_ui"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="UI"
        />

    <Button
        android:id="@+id/btn_H5_webPage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="H5和网页"
        />
</LinearLayout>
